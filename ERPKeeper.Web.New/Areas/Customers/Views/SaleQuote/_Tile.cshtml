@model ERPKeeperCore.Enterprise.Models.Customers.SaleQuote



<div class="grid grid-cols-6">
    <a href="/@CompanyId/Customers/SaleQuotes/@Model.Id">
        <div class="bg-gray-100 h-20 content-center border">
            <div class="text-3xl text-center font-semibold">
                @Model.Status
            </div>
            <div class="text-lg text-center">
                Status
            </div>
        </div>
    </a>


    <a href="/@CompanyId/Customers/SaleQuotes/@Model.Id/SaleQuotes">
        <div class="bg-gray-100 h-20 content-center border">
            <div class="text-3xl text-center font-semibold">
                @Model.AgeInDays
            </div>
            <div class="text-lg text-center">
                Age
            </div>
        </div>
    </a>


    <a href="/@CompanyId/Customers/SaleQuotes/@Model.Id/SaleQuotes">
        <div class="bg-gray-100 h-20 content-center border">
            <div class="text-3xl text-center font-semibold">

              </div>
            <div class="text-lg text-center">
                
            </div>
        </div>
    </a>


    <a href="/@CompanyId/Customers/SaleQuotes/@Model.Id/Items">
        <div class="bg-gray-100 h-20 content-center border">
            <div class="text-3xl text-center font-semibold">
                @Model.Items.Count()
            </div>
            <div class="text-lg text-center">
                Items
            </div>
        </div>
    </a>



</div>


