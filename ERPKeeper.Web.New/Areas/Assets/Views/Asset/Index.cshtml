@model ERPKeeper.Node.Models.Assets.FixedAsset

<div class="row">
    <div class="col-8 p-0 col-center mh-100v border-right border-2">
        <partial name="_MenuTop" />
        <partial name="_EditForm" />

        @if (Model.FixedAssetType.DeprecatedAble)
        {
            <partial name="_DeprecateSchedule_Grid" />
        }

    </div>
    <div class="col-4 pr-0 col-right">
        @await Component.InvokeAsync("_Journal_Accounting",new{companyId = CompanyId, journalId = Model.Uid})
    </div>
</div>

