@model ERPKeeperCore.Enterprise.Models.Profiles.Profile


<div class="flex border">
    <div class="col p-0">


        @{
            if (Model.Customer != null)
            {
                <div class="p-2 border float-left">
                    <a href="/tec/Customers/Customers/@Model.Id">Customer</a>
                </div>
            }
            if (Model.Supplier != null)
            {
                <div class="p-2 border float-left">
                    <a href="/tec/Suppliers/Suppliers/@Model.Id">Supplier</a>
                </div>
            }
            if (Model.Employee != null)
            {
                <div class="p-2 border float-left">
                    <a href="/tec/Employees/Employees/@Model.Id">Employee</a>
                </div>
            }
            if (Model.Investor != null)
            {
                <div class="p-2 border float-left">
                    <a href="/tec/Investors/Investors/@Model.Id">Investor</a>
                </div>
            }
        }



    </div>
</div>

