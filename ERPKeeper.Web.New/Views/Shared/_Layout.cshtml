<!DOCTYPE html>
<html>
<head>
    <partial name="_Header_CSS_Font" />
    <partial name="_Header_CSS" />
</head>
<body class="dx-viewport lh-12  h-screen" style="height:100%;">
    <div id="GlobalContainer">
        <div class="flex bg-gray-100">
            <div class="w-60 border flex-shrink-0">
                <div class="lh-120 text-xl">
                    <partial name="_Header_logo" />
                </div>
                <div class="side-nav-buttom">
                </div>
            </div>
            <div class="grow  p-0 border">
                <div class="flex ">
                    <div class="w-9/12 p-0">
                        <partial name="_MenuTop" />
                    </div>
                    <div class="w-3/12 p-2">
                        <partial name="_DropMenu" />
                    </div>
                </div>
            </div>
        </div>
        <div class="flex">
            <div class="w-60  border flex-shrink-0">
                <div class="lh-120 text-xl">
                    <partial name="_LeftMenu" />
                </div>
                <div class="side-nav-buttom">
                </div>
            </div>
            <div class="grow  p-0">
                @RenderBody()
            </div>
        </div>
    </div>



</body>
</html>