@model ERPKeeper.Node.Models.Items.GroupItem

<div class="globalContainer">
    <div class="row">
        <div class="col-2 col-Left pl-0">
            @Html.Action("_Main", "Default", new { Area = "Menu" })
        </div>
        <div class="col col-Middle p-0">
            @Html.Partial("_MenuTop")
            @Html.Partial("_Detail", Model)
            @Html.Action("PartialGridView")

        </div>
        <div class="col-3 col-Right pr-0">
            <div class="mb-2 mh-80 bg-fb2 p-2">

            </div>
            @Html.Partial("_CreateChildItem")
            @Html.Partial("_AddItem")
            @Html.Partial("_Parameters", Model)
        </div>
    </div>
</div>