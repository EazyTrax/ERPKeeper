@model ERPKeeper.Node.Models.Assets.FixedAssetType


<div class="globalContainer">
    <div class="row">
        <div class="col-2 col-Left pl-0">
            @Html.Action("_Main", "Default", new { Area = "Menu" })


        </div>
        <div class="col-4 col-Middle pr-0">
            @Html.Partial("_MenuTop")
            @Html.Partial("_Detail", Model)
        </div>
        <div class="col-3 col-Right pr-0">
            <div class="col p-2 mb-1 bg-fb10">

            </div>
        </div>
        <div class="col-3 col-Right pr-0">
            @if (Model?.PurchaseAccUid != null)
            {
                @Html.Action("_MiniChart", "AccountItem", new { Area = "Accounting", id = Model.PurchaseAccUid })
            }
        </div>
    </div>
</div>
