@model ERPKeeper.Node.Models.Financial.Lends.Lend


<div class="globalContainer">
    <div class="row">
        <div class="col-2 col-Left pl-0">
            @if (Model != null)
            {
                @Html.Partial("_MenuTop", Model)
            }
            else
            {
                <span style="font-weight: 600;">
                    NEW Lend
                </span>
            }

        </div>
        <div class="col col-Middle p-0">
            <div class="bg-fb2 p-2 mb-2 mh-50">
                @Model.Name
            </div>
            @Html.Partial("_Detail", Model)

        </div>
    </div>
</div>
