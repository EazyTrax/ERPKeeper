
@model ERPKeeper.Node.Models.Accounting.AccountItem

<div class="globalContainer">
    <div class="row">
        <div class="col-2 col-Left pl-0">
            @Html.Partial("_MenuTop")
        </div>
        <div class="col col-Middle p-0">
            @Html.Action("PartialGridViewPayments", "LiabilityAccount", new { id = Model.Uid })
        </div>
        <div class="col-3 col-Right pr-0">
            @Html.Action("_MiniChart", "AccountItem", new { Area = "Accounting", id = Model.Uid })
        </div>
    </div>
</div>


