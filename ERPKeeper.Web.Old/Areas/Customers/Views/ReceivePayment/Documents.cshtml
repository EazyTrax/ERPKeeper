@model ERPKeeper.Node.Models.Financial.Payments.GeneralPayment

<div class="globalContainer">
    <div class="row">
        <div class="col-2 col-Left pl-0">
                @Html.Action("_Main", "Default", new { Area = "Menu" })
            </div>
            <div class="col-7">
                @Html.Partial("_MenuTop")

                @Html.Action("_List", "Documents", new { Area = "Documents", id = Model.Uid })
            </div>

            <div class="col-3 col-Right pr-0">
                @Html.Action("_FileUploadForm", "Documents", new { Area = "Documents", id = Model.Uid })
            </div>
        </div>
</div>
